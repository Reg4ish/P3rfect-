return function(io)
	local h = io[1].Character.HumanoidRootPart
	local s, fz = h.CFrame, io[2].e.Value
	io[1].Character.Humanoid.Sit = false
	io[2].e:SetValue(true)
	local toclaim = {}
	for _,v in pairs(workspace.active:GetChildren()) do
		if v.Name == io[1].Name and v.Prompt.Enabled then
			table.insert(toclaim, v)
		end
	end
	io[3]("Found " .. #toclaim .. " CC To Claim!")
	local function temp()
		for _,v in pairs(toclaim) do
			if not v.PrimaryPart then
				continue
			end
			h.CFrame = CFrame.new(v.PrimaryPart.Position) + Vector3.new(0, 4, 0)
			local p = v:FindFirstChild("Prompt")
			if (not p or not v:FindFirstChild("Highlight")) then
				continue
			end			
			local function temp()				
				p.MaxActivationDistance = math.huge
				p.HoldDuration = 0
				p.RequiresLineOfSight = false
				fireproximityprompt(p)
				if v.Parent == workspace.active and v.PrimaryPart then
					task.wait()
					return temp()
				end
				table.remove(toclaim, table.find(toclaim, v))
			end
			temp()
		end
		if #toclaim ~= 0 then
			task.wait()
			temp()
		end
	end
	temp()
	h.CFrame = s
	if not fz then
		io[2].e:SetValue(false)
	end
end
